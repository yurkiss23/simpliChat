<Window x:Class="simpliChat.Windows.SelectWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:simpliChat.Windows"
        mc:Ignorable="d"
        Title="SelectWindow" Height="100" Width="300" WindowStartupLocation="CenterScreen"
        Loaded="Window_Loaded">
    <StackPanel>
        <Label x:Name="lblSelName" Content="Input Name:" HorizontalAlignment="Center"/>
        <TextBox x:Name="txtSelName" HorizontalAlignment="Center" HorizontalContentAlignment="Center"/>
    </StackPanel>
</Window>
